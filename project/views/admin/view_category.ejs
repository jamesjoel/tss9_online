<div class="row mt-3">
    <div class="col-md-8 offset-md-2">
        <h3 class="text-center">
            View All Categories
        </h3>
        <table class="table table-dark table-hover table-bordered mt-2">
            <tr>
                <th>S.No.</th>
                <th>Category Name</th>
            </tr>
            <% result.forEach(function(cate, index){ %>
                <tr>
                    <td><%= index+1 %></td>
                    <td><%= cate.name %></td>
                </tr>
            <% }) %>
        </table>
    </div>
</div>