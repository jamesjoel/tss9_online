<div class="container" style="min-height: 650px;">
    <h3 class="mt-4">Student Information</h3>
    
    <div class="row">
        <div class="col-md-8 offset-md-2">
            <div class="card">
                <div class="card-body">
                    <table class="table table-bordered">
                        <tr>
                            <td>Student Name</td>
                            <td><%= result.name %></td>
                        </tr>
                        <tr>
                            <td>Student Age</td>
                            <td><%= result.age %></td>
                        </tr>
                        <tr>
                            <td>Student City</td>
                            <td><%= result.city %></td>
                        </tr>
                        <tr>
                            <td>Student Address</td>
                            <td><%= result.address %></td>
                        </tr>
                        <tr>
                            <td>Student Fee</td>
                            <td><%= result.fee %></td>
                        </tr>
                        <tr>
                            <td>Student Gender</td>
                            <td><%= result.gender %></td>
                        </tr>
                    </table>
                </div>
        
            </div>
        </div>
    </div>

</div>


